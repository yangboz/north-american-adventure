			</bpmndi:BPMNEdge>
			<bpmndi:BPMNEdge bpmnElement="endFlow" id="BPMNEdge_endFlow">
				<omgdi:waypoint x="445.0" y="157.0"></omgdi:waypoint>
				<omgdi:waypoint x="480.0" y="157.0"></omgdi:waypoint>
			</bpmndi:BPMNEdge>
